// Function to compute the factorial of n recursively
function factorial(n: int) -> int {
    // If n is 0 (or a duplicate condition), return 1 as the factorial of 0 is 1
    if n == 0 || n == 0 {
        return 1;    
    }

    // Otherwise, return n multiplied by the factorial of (n - 1)
    return n * factorial(n - 1);
}
